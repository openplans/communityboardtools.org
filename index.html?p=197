  jQuery(function ($) {
    $(document).ready(function() {

      $('#nav-bttn').click(function(){
        $('#sidebar-nav').toggleClass('expose');
        return false; 
      });

      // toggle search text
      $("#s").val('Search...').addClass('placeholder');
      $('#s').focus(function(){ 
        if($(this).val() == 'Search...' ) {
          $(this).val('').removeClass('placeholder');
        }
      });
      $('#s').blur(function(){
        if($(this).val() == '') {
          $(this).val('Search...').addClass('placeholder');
        } 
      });

    });
  });
</script>

